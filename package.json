{
  "name": "tplink-mcu",
  "version": "0.0.0",
  "private": true,
  "description": "Control tplink smart light devices with nodemcu",
  "scripts": {
    "setup": "nodemcu-tool upload examples/wifi.lua examples/settings.lua",
    "example:alloff": "nodemcu-tool upload examples/alloff.lua --remotename init.lua",
    "example:allon": "nodemcu-tool upload examples/allon.lua --remotename init.lua",
    "terminal": "nodemcu-tool reset && nodemcu-tool terminal"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konsumer/tplink-mcu.git"
  },
  "keywords": [],
  "author": "David Konsumer <konsumer@jetboystudio.com> (http://konsumer.js.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/konsumer/tplink-mcu/issues"
  },
  "homepage": "https://github.com/konsumer/tplink-mcu#readme",
  "dependencies": {
    "nodemcu-tool": "^2.1.1",
    "npm": "^6.0.0"
  }
}
